#include <bits/stdc++.h>
using namespace std;

typedef long long ll;
typedef pair<int, int> ii;
typedef vector<int> vi;
typedef vector<ll> vll;
typedef vector<ii> vii;
typedef vector<vi> vvi;

const int INF = 1e9;
const ll LLINF = 1e18;
const int MOD = 1e9 + 7;
const double EPS = 1e-9;

const int N = 1e6 + 5;
int spf[N], numDiv[N], spfExp[N];
vi primes;

void sieve() {
  spf[1] = 1; numDiv[1] = 1;
  for (int i = 2; i < N; i++) {
    if (spf[i] == 0) {
      spf[i] = i;
      primes.push_back(i);
    }
    for (int& p : primes) {
      if (p > spf[i] || (ll) p * i >= N) break;
      int v = p * i;
      spf[v] = p;

      if (spf[v] == p) {
        spfExp[v] = spfExp[i] + 1;
        int k = spfExp[i];
        numDiv[v] = numDiv[i] / (k + 1) * (k + 2);
      }
      else {
        spfExp[v] = 1;
        numDiv[v] = numDiv[i] * 2;
      }
    }
  }
}

int main() {
  ios::sync_with_stdio(0); cin.tie(0);
  sieve();
  int a, b, c; cin >> a >> b >> c;

  ll ans = 0;
  for (int i = 1; i <= a; i++)
    for (int j = 1; j <= b; j++)
      for (int k = 1; k <= c; k++) 
        ans = (ans + numDiv[i * j * k]) % MOD;

  cout << ans;

  return 0;
}
